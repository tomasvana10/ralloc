{
  "name": "ralloc",
  "version": "0.23.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "prepare": "next-ws patch && [ -d .git ] && lefthook install || echo \"no lefthook in container\"",
    "deploy": "pnpm run build && pnpm run start",
    "d:dev": "docker compose -f .docker/compose.dev.yaml up --build",
    "d:prod-local": "docker compose -f .docker/compose.prod-local.yaml up --build",
    "d:prod-ci": "docker compose -f .docker/compose.prod-ci.yaml pull && docker compose -f .docker/compose.prod-ci.yaml up --build -d"
  },
